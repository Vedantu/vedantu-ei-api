<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>vedantu-ei-api</display-name>

	<servlet>
		<description></description>
		<display-name>AuthServlet</display-name>
		<servlet-name>AuthServlet</servlet-name>
		<servlet-class>com.vedantu.ei.servlets.AuthServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AuthServlet</servlet-name>
		<url-pattern>/authenticate</url-pattern>
	</servlet-mapping>


	<servlet>
		<description></description>
		<display-name>UploadTestAttemptsServlet</display-name>
		<servlet-name>UploadTestAttemptsServlet</servlet-name>
		<servlet-class>com.vedantu.ei.servlets.UploadTestAttemptsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UploadTestAttemptsServlet</servlet-name>
		<url-pattern>/uploadTestAttemptData</url-pattern>
	</servlet-mapping>

</web-app>
